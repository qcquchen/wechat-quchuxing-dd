<view class="body">
  <view wx:for="{{ driverList }}" wx:for-item="items" wx:key="{{index}}" bindtap="gotoInfo" data-id="{{items.phone}}">
    <view class="auditList">
      <view class="list_left">
        <view class="name">{{ items.nickName }}</view>
        <view class="time">{{ items.creatTime }}</view>
      </view>
      <view class="list_right">
        <view class="text">{{ items.status }}</view>
        <image src="../../images/icon_return_left@3x.png" class="right_icon"></image>
      </view>
    </view>
  </view>
</view>
